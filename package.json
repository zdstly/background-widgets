{
	"name": "aladdin-chewan-plugin",
	"displayName": "background widgets",
	"description": "A simple tool to make your vscode's background look better!",
	"version": "0.0.4",
	"publisher": "aladdin",
	"author": "aladdin",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"keywords": [
		"车万",
		"东方",
		"wallpaper",
		"background",
		"pomodoro",
		"timer",
		"productivity"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "lovely background",
			"properties": {
				"frankenstein.reminderViewIntervalInMinutes": {
					"type": "number",
					"default": 25,
					"description": "展示提醒页面的时间间隔（分钟）"
				}
			}
		}
	},
	"icon": "cover.png",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"fs-extra": "^9.0.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"fs-extra": "^9.0.0",
		"@types/fs-extra": "^8.1.0"
	}
}